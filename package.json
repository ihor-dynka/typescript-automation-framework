    "scripts": {
        "apiTest": "mocha --config ./config/mocha/.mocharc.api.json --reporter mocha-junit-reporter --reporter-options mochaFile=./report/api/junit.xml",
        "startSelenoid": "docker-compose --file selenoid.yml up -d",
        "stopSelenoid": "docker-compose --file selenoid.yml down",
        "webTest": "mocha --config ./config/mocha/.mocharc.web.json",
        "allTest": "npm run apiTest && npm run webTest",
        "eslint": "eslint . --ext .ts,.js --ignore-pattern node_modules"
    },
