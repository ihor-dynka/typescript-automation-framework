        "dotenv": "^10.0.0",
        "envalid": "^7.2.1",
        "got": "^11.8.2",
        "mocha": "^9.2.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4",
        "webdriverio": "^7.16.13"
    },
    "devDependencies": {
        "@types/chromedriver": "^81.0.0",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
